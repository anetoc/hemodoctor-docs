{
  "commands": {
    "/comprehensive-review": {
      "description": "Revisão completa 360° de toda documentação e contexto",
      "usage": "/comprehensive-review [escopo] [profundidade]",
      "examples": [
        "/comprehensive-review todos profunda",
        "/comprehensive-review PKG01-05 compliance-focused"
      ],
      "output": "REV-001 com status detalhado de conformidade e gaps"
    },
    "/gap-analysis": {
      "description": "Análise sistemática de lacunas por categoria",
      "usage": "/gap-analysis [categoria] [baseline]",
      "examples": [
        "/gap-analysis regulatória RDC657+751",
        "/gap-analysis técnica IEC62304+ISO14971"
      ],
      "output": "GAP-001 com lacunas identificadas, impacto e ações corretivas"
    },
    "/context-alignment": {
      "description": "Validação de alinhamento com orientações iniciais",
      "usage": "/context-alignment [orientacoes] [ativos]",
      "examples": [
        "/context-alignment iniciais 1.5M-aproveitamento",
        "/context-alignment baseline-tecnica sistema-producao"
      ],
      "output": "CTX-001 verificando aderência total às orientações originais"
    },
    "/checklist-master": {
      "description": "Checklist master detalhado de todos os documentos",
      "usage": "/checklist-master [documentos] [compliance]",
      "examples": [
        "/checklist-master 67-docs ANVISA-ready",
        "/checklist-master PKG01-14 submission-ready"
      ],
      "output": "CHK-001 checklist com status por documento e % completude"
    },
    "/quality-gate": {
      "description": "Avaliação de quality gates regulatórios",
      "usage": "/quality-gate [nivel] [criterios]",
      "examples": [
        "/quality-gate submission-ready ANVISA-criteria",
        "/quality-gate alpha-checkpoint foundation-complete"
      ],
      "output": "QUA-001 status aprovação quality gates com recomendações"
    },
    "/submission-readiness": {
      "description": "Avaliação completa de prontidão para submissão",
      "usage": "/submission-readiness [alvo] [completude]",
      "examples": [
        "/submission-readiness ANVISA 100%-compliance",
        "/submission-readiness FDA+ANVISA international-ready"
      ],
      "output": "SUB-001 relatório prontidão com checklist final"
    },
    "/cross-validation": {
      "description": "Validação cruzada entre documentos e consistência",
      "usage": "/cross-validation [documentos] [dependencias]",
      "examples": [
        "/cross-validation PKG01-14 dependencias-mapeadas",
        "/cross-validation regulatorio+tecnico+clinico consistency-check"
      ],
      "output": "Relatório inconsistências, conflitos e recomendações"
    },
    "/asset-utilization": {
      "description": "Validação aproveitamento ativos existentes R$ 1.5M",
      "usage": "/asset-utilization [ativos] [roi]",
      "examples": [
        "/asset-utilization 1.5M-existing ROI-458%",
        "/asset-utilization dossiers-existentes maximum-reuse"
      ],
      "output": "Relatório otimização, aproveitamento e economia validada"
    },
    "/compliance-audit": {
      "description": "Auditoria completa de conformidade regulatória",
      "usage": "/compliance-audit [normas] [evidencias]",
      "examples": [
        "/compliance-audit RDC657+751+ISO13485 todas-evidencias",
        "/compliance-audit ANVISA-requirements submission-package"
      ],
      "output": "Relatório auditoria com score conformidade e ações"
    },
    "/timeline-validation": {
      "description": "Validação realística do timeline de 16 meses",
      "usage": "/timeline-validation [marcos] [recursos]",
      "examples": [
        "/timeline-validation 16-meses quality-gates",
        "/timeline-validation clinical-studies realistic-assessment"
      ],
      "output": "Validação timeline com riscos e contingências"
    },
    "/stakeholder-alignment": {
      "description": "Verificação alinhamento com todos stakeholders",
      "usage": "/stakeholder-alignment [grupos] [expectativas]",
      "examples": [
        "/stakeholder-alignment todos expectativas-iniciais",
        "/stakeholder-alignment clinical+regulatory+technical consensus"
      ],
      "output": "Relatório alinhamento com ações para consenso"
    },
    "/risk-benefit-review": {
      "description": "Revisão global da análise risco-benefício",
      "usage": "/risk-benefit-review [escopo] [populacao]",
      "examples": [
        "/risk-benefit-review global adulto+pediatrico",
        "/risk-benefit-review clinical-utility patient-safety"
      ],
      "output": "Validação risco-benefício com justificativa regulatória"
    }
  }
}