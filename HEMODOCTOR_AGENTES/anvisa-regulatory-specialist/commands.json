{
  "commands": {
    "/reg-strategy": {
      "description": "Gerar estratégia regulatória completa (REG-001)",
      "usage": "/reg-strategy [classe] [população]",
      "examples": [
        "/reg-strategy III adulto+pediatrico",
        "/reg-strategy conservador população-geral"
      ],
      "output": "Documento REG-001 com classificação, intended use, timeline aprovação"
    },
    "/gap-analysis": {
      "description": "Análise de conformidade RDC 657/2022 (REG-002)",
      "usage": "/gap-analysis [norma] [escopo]",
      "examples": [
        "/gap-analysis RDC657 completa",
        "/gap-analysis RDC751 classificação"
      ],
      "output": "Gap analysis detalhado com ações corretivas e prazos"
    },
    "/consulta-anvisa": {
      "description": "Preparar consulta técnica ANVISA (REG-004)",
      "usage": "/consulta-anvisa [tema] [urgência]",
      "examples": [
        "/consulta-anvisa classificação crítica",
        "/consulta-anvisa evidências normal"
      ],
      "output": "Pacote completo para consulta técnica ANVISA"
    },
    "/intended-use": {
      "description": "Definir intended use statement preciso",
      "usage": "/intended-use [população] [ambiente] [limitações]",
      "examples": [
        "/intended-use geral laboratório triagem",
        "/intended-use adulto+pediatrico hospitalar apoio-decisao"
      ],
      "output": "Intended use otimizado para submissão regulatória"
    },
    "/regulatory-intel": {
      "description": "Inteligência regulatória atualizada (REG-003)",
      "usage": "/regulatory-intel [região] [produto-tipo]",
      "examples": [
        "/regulatory-intel brasil CDSS-hematologia",
        "/regulatory-intel latam software-medico"
      ],
      "output": "Relatório de mudanças regulatórias e impactos"
    },
    "/submission-package": {
      "description": "Preparar pacote de submissão ANVISA",
      "usage": "/submission-package [tipo] [classificação]",
      "examples": [
        "/submission-package inicial classe-III",
        "/submission-package atualizacao classe-II"
      ],
      "output": "Checklist e documentos prontos para submissão"
    },
    "/classification-analysis": {
      "description": "Análise detalhada de classificação de risco",
      "usage": "/classification-analysis [produto] [uso-pretendido]",
      "examples": [
        "/classification-analysis CDSS triagem-CBC",
        "/classification-analysis software apoio-diagnóstico"
      ],
      "output": "Justificativa técnica da classificação com RDC 751"
    },
    "/compliance-check": {
      "description": "Verificação de conformidade regulatória",
      "usage": "/compliance-check [norma] [documentos]",
      "examples": [
        "/compliance-check RDC657 todos",
        "/compliance-check ISO13485 específicos"
      ],
      "output": "Status de conformidade com ações necessárias"
    }
  }
}